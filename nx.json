{
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["{projectRoot}/dist", "{projectRoot}/build"],
			"cache": true
		},
		"check": {
			"cache": true
		},
		"check:watch": {
			"cache": true
		},
		"lint": {
			"cache": true
		},
		"format": {
			"cache": true
		},
		"test": {
			"cache": true
		}
	},
	"namedInputs": {
		"default": ["{projectRoot}/**/*", "sharedGlobals"],
		"sharedGlobals": [],
		"production": ["default"]
	}
}
