{
	"name": "@greendoc/svelte",
	"version": "0.3.0",
	"repository": "https://github.com/donmccurdy/greendoc",
	"author": "Don McCurdy <dm@donmccurdy.com>",
	"license": "Prosperity-3.0.0",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"Class.svelte": [
				"./dist/Class.d.ts"
			],
			"Constructor.svelte": [
				"./dist/Constructor.d.ts"
			],
			"Comment.svelte": [
				"./dist/Comment.d.ts"
			],
			"Enum.svelte": [
				"./dist/Enum.d.ts"
			],
			"EnumMember.svelte": [
				"./dist/EnumMember.d.ts"
			],
			"Function.svelte": [
				"./dist/Function.d.ts"
			],
			"Interface.svelte": [
				"./dist/Interface.d.ts"
			],
			"Method.svelte": [
				"./dist/Method.d.ts"
			],
			"Property.svelte": [
				"./dist/Property.d.ts"
			],
			"Reference.svelte": [
				"./dist/Reference.d.ts"
			],
			"Sources.svelte": [
				"./dist/Sources.d.ts"
			],
			"index": [
				"./dist/index.d.ts"
			]
		}
	},
	"scripts": {
		"dev:disabled": "vite dev --force",
		"build": "svelte-kit sync && svelte-package -o dist",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check src",
		"format": "prettier --write src",
		"test": "echo \"Tests not yet implemented\"",
		"preversion": "yarn build && yarn test",
		"version": "rimraf dist/* && yarn build && git add -u",
		"postversion": "git push && git push --tags && npm publish --access public",
		"clean": "rimraf dist/*"
	},
	"dependencies": {
		"@greendoc/parse": "^0.3.0"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.1",
		"@sveltejs/kit": "1.27.6",
		"@sveltejs/package": "^2.2.3",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.1"
	},
	"files": [
		"dist"
	],
	"gitHead": "276870f41d0e92060bb04b138d1ee8fbe643d1d3"
}
