{
	"name": "@greendoc/svelte",
	"version": "0.1.5",
	"repository": "https://github.com/donmccurdy/greendoc",
	"author": "Don McCurdy <dm@donmccurdy.com>",
	"license": "UNLICENSED",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"Class.svelte": [
				"./dist/Class.d.ts"
			],
			"Comment.svelte": [
				"./dist/Comment.d.ts"
			],
			"Enum.svelte": [
				"./dist/Enum.d.ts"
			],
			"EnumMember.svelte": [
				"./dist/EnumMember.d.ts"
			],
			"Function.svelte": [
				"./dist/Function.d.ts"
			],
			"Interface.svelte": [
				"./dist/Interface.d.ts"
			],
			"Method.svelte": [
				"./dist/Method.d.ts"
			],
			"Property.svelte": [
				"./dist/Property.d.ts"
			],
			"Reference.svelte": [
				"./dist/Reference.d.ts"
			],
			"Sources.svelte": [
				"./dist/Sources.d.ts"
			],
			"index": [
				"./dist/index.d.ts"
			]
		}
	},
	"scripts": {
		"dev": "vite dev --force",
		"watch": "svelte-kit sync && svelte-package -o dist --watch",
		"build": "svelte-kit sync && svelte-package -o dist",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check src",
		"format": "prettier --plugin-search-dir . --write src",
		"test": "echo \"Tests not yet implemented\"",
		"preversion": "yarn build && yarn test",
		"version": "rimraf dist/* && yarn build && git add -u",
		"postversion": "git push && git push --tags && npm publish --access public",
		"clean": "rimraf dist/*"
	},
	"dependencies": {
		"@greendoc/parse": "^0.1.5"
	},
	"peerDependencies": {
		"svelte": "^3.59.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.0.1",
		"@sveltejs/kit": "1.16.3",
		"@sveltejs/package": "^2.0.0",
		"svelte-check": "^3.3.2",
		"svelte-preprocess": "^5.0.3"
	},
	"files": [
		"dist"
	],
	"gitHead": "032c6e5fc3a42b3bc947216bea11b446aea10493"
}
